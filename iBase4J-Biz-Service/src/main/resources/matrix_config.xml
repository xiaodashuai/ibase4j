<WORKFLOW>
	<APPSERVER>
		<APPSERVER>Tomcat</APPSERVER>
		<IS_CLUSTERED>false</IS_CLUSTERED>
	</APPSERVER>
	<!-- pointbase -->
	<JDBC>
		<JDBC_RDBMS_TYPE>Oracle</JDBC_RDBMS_TYPE>
		<JDBC_DS_NAMING>dataSource</JDBC_DS_NAMING>
	</JDBC>

	<MODE>
		<TYPE>DEVELOPMENT</TYPE>
	</MODE>

	<SEQUENCE>
		<SEQUENCE_TYPE>ByDB</SEQUENCE_TYPE>
	</SEQUENCE>

	<AUDIT>
		<AUDIT_ENABLE>true</AUDIT_ENABLE>
		<KEEP_RELEVANT_WORKITEMS>false</KEEP_RELEVANT_WORKITEMS>
		<MOVETOHISTORY_ENABLE>false</MOVETOHISTORY_ENABLE>
	</AUDIT>

	<SESSION>
		<SESSION_EXPIRY_TIME>1440</SESSION_EXPIRY_TIME>
		<SESSION_MONITOR_TIME>5</SESSION_MONITOR_TIME>
	</SESSION>

	<CLEANUP>
		<ENABLE>true</ENABLE>
		<MONITOR_INSTANCE_HOUR>5</MONITOR_INSTANCE_HOUR>
		<REMAIN_INSTANCE_DAY>2</REMAIN_INSTANCE_DAY>
		<MONITOR_TEMPLATE_DAY>24</MONITOR_TEMPLATE_DAY>
	</CLEANUP>

	<ORGANIZATION>
		<STORAGE_TYPE>RDBMS</STORAGE_TYPE>
		<STORAGE_IMPL></STORAGE_IMPL>
		<JDBC_DS_NAMING>dataSource</JDBC_DS_NAMING>
	</ORGANIZATION>

	<AUTHENTICATION>
		<LOGIN_MODULE>RDBMS</LOGIN_MODULE>
		<IS_EXTERNAL>false</IS_EXTERNAL>
		<PASSWORD_CRYPTO_IMPL>com.matrix.engine.foundation.domain.CryptoBase64Impl
		</PASSWORD_CRYPTO_IMPL>
	</AUTHENTICATION>
	<EVENT>
		<IS_ENABLE>false</IS_ENABLE>
		<THREAD_POOL_SIZE>5</THREAD_POOL_SIZE>
	</EVENT>

	<SYSTEM>
		<ADMIN_ROLE>1</ADMIN_ROLE>
		<SYSTEM_USER>admin</SYSTEM_USER>
	</SYSTEM>

</WORKFLOW>
